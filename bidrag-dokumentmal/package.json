{
  "name": "bidrag-dokumentmal",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run tailwind_build & remix vite:build",
    "dev2": "run-p dev:*",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "icons": "npx @svgr/cli --out-dir app/components/icons -- app/icons",
    "icons:watch": "npm-watch icons",
    "dev:svg": "run-s icons icons:watch",
    "dev": "remix dev",
    "tailwind_dev": "tailwindcss build -i app/tailwind.css -o app/style/tw_output.css --watch",
    "tailwind_build": "tailwindcss build -i app/tailwind.css -o app/style/tw_output.css",
    "gen_api": "swagger-typescript-api -p http://localhost:8183/v3/api-docs --no-client -n Api.ts -o app/types"
  },
  "dependencies": {
    "@navikt/aksel-icons": "^7.9.0",
    "@navikt/ds-css": "^7.9.0",
    "@navikt/ds-react": "^7.9.0",
    "@remix-run/css-bundle": "^2.17.0",
    "@remix-run/node": "^2.17.0",
    "@remix-run/react": "^2.17.0",
    "@remix-run/serve": "^2.17.0",
    "isbot": "^5.1.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remix": "^2.17.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.16.2",
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.13.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "swagger-typescript-api": "^13.0.22",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "5.4.20",
    "vite-plugin-singlefile": "^2.1.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "watch": {
    "icons": {
      "patterns": [
        "icons"
      ],
      "extensions": "svg",
      "quiet": false
    }
  }
}
